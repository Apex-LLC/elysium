<table>
  <thead>
    <tr>
      <th>Symbol</th>
      <th>Rate</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% if (rates != nil)
      rates.each do |rate| %>
        <tr>
          <td><%= rate.symbol %></td>
          <td><%= rate.rate %></td>
          <td><%= rate.description %></td>
          <td><%= link_to 'Edit', edit_rate_path(rate) %></td>
          <td><%= link_to 'âœ•', rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>